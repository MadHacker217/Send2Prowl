		<em:optionsURL>chrome://prowlingfirefox/content/options.xul</em:optionsURL>

		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>2.0</em:minVersion>
				<em:maxVersion>5.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>      
</RDF>
